<div align="Center">
  <% if user_signed_in? %>
    <div>
      <h1>Welcome <%= current_user.email %></h1>
    </div>
    <%= button_to "Sign out", destroy_user_session_path, method: :delete %>
    <h2><a class="btn" href="/clinics">Go to clinics</a></h2>
  <% else %>
    <%= button_to "Sign in", new_user_session_path %>
  <% end %>
</div>
